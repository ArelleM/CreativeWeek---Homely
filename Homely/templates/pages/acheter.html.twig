{% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/all.css" integrity="sha384-Bfad6CLCknfcloXFOyFnlgtENryhrpZCe29RTifKEixXQZ38WheV+i/6YWSzkz3V" crossorigin="anonymous">
{% endblock %}

<body>
<nav>

    <div class="nav-bar" >
        <ul class="navbar-ul">
            <a href="{{ path('home') }}">
                <img id="logoImg" src="/images/logo/HomelyV2.png" alt="Logo Homely">
            </a>
        </ul>
        <div class="navbar-right">
            <ul class="navbar-ul liens">
                <span style="padding-right: 2em;"><i class="fas fa-globe-americas"></i> <i class="fas fa-chevron-down"></i></span>
                <a class="cool-link" href="{{ path('locations') }}"> Louer un bien</a>
                <a class="cool-link" href="{{ path('acheter') }}"> Acheter un bien</a>
                <a class="cool-link" href="{{ path('apropos') }}"> A propos</a>
            </ul>
            {%  if app.user %}
                <div class="card" id="userBlock">
                    <div class="card-body user">
                        <h5 class="card-text w-5">
                            <span class="fa fa-folder-o"></span>
                            <a href="#menu" data-toggle="collapse">
                                <span> {{ app.user.username }}</span>
                                <img id="userImg" src="/images/Users/default_user.png" alt="Image Utilisateur Défault">
                            </a>
                        </h5>
                    </div>
                    <div id="menu" class="collapse">
                        <div class="list-group ">
                            <a href="#" class="text-white">Mon profil</a>
                            <a href="{{ path('app_logout') }}" class="text-white pb-2">Se déconnecter</a>
                        </div>
                    </div>
                </div>

            {% else %}

                <ul class="navbar-ul connexion">
                    <div id="nav-connect">
                        <li class="nav-item">
                            <a class="cool-link connect" href="{{ path('app_login') }}" class="cool-link">Se connecter</a>
                        </li>
                        <li class="nav-item">
                            <a class="cool-link" href="{{ path('register') }}" class="cool-link">S'inscrire</a>
                        </li>
                    </div>

                </ul>

            {% endif %}

        </div>
    </div>
</nav>
<div class=" container bookingform mt-2">

    <form class="form-inline w-100">
        <label class="sr-only" for="inlineFormInputName2">Destination</label>
        <input type="text" class="formresearch form-control form-control-lg mr-sm-2" id="inlineFormInputName2" placeholder="Destination">

        <label class="sr-only" for="inlineFormInputGroupUsername2">Date d'arrivée</label>
        <div class="input-group mr-sm-2">
            <div class="input-group-prepend">
            </div>
            <input type="date" class="formresearch form-control form-control-lg" >
        </div>
        <label class="sr-only" for="inlineFormInputGroupUsername2">Date de départ</label>
        <div class="input-group mr-sm-5">
            <div class="input-group-prepend">
            </div>
            <input type="date" class=" formresearch form-control form-control-lg" >
        </div>


        <button type="submit" id="rechercher" class="btn btn-primary btn-lg ">
            <h6>Rechercher</h6>
            <i class="fas fa-search"></i>

        </button>
    </form>
</div>
